<launch>

 <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen">
       <param name="video_device" value="/dev/video1" />
        <param name="image_width" value="320" /> #max: 2304
        <param name="image_height" value="240" /> #max: 1536
        <param name="pixel_format" value="mjpeg" />
        <param name="camera_frame_id" value="webcam" />
        <param name="camera_info_url" type="string" value="$(find sparki_bringup)/data/calibration_data/ost.yaml" />

	<remap from="/usb_cam/image_raw" to="/image_raw"/>
	<remap from="/usb_cam/camera_info" to="/camera_info"/>

    </node>


<node name="image_proc" pkg="image_proc" type = "image_proc" />


</launch>
